<Application x:Class="iDuel_EvolutionX.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             
             StartupUri="MainWindow.xaml">
    <Application.Resources>
        <!-- 应该在此定义应用程序级的资源。-->
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <ResourceDictionary Source="UI/ResourceDictionary/MyTabControl.xaml"/>
                <ResourceDictionary Source="UI/ResourceDictionary/TextBlockNum.xaml"/>
                <ResourceDictionary Source="UI/ResourceDictionary/MyBorder.xaml"/>
                <ResourceDictionary Source="UI/ResourceDictionary/MyMenuItem.xaml"/>
            </ResourceDictionary.MergedDictionaries>
        </ResourceDictionary>
    </Application.Resources>
</Application>
